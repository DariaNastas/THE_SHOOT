  }
}

.talents__image {
  justify-content: center;
  width: 50%;
  padding-top: 172px;
  display: flex;
  position: relative;
}

@media (width >= 769px) and (width <= 1024px) {
  .talents__image {
    justify-content: end;
    padding-top: 18%;
  }
}

@media (width <= 768px) {
  .talents__image {
    justify-content: end;
    padding-top: 15%;
  }
}

@media (width <= 426px) {
  .talents__image {
    justify-content: end;
    padding-top: 35%;
  }
}

@media (width <= 320px) {
  .talents__image {
    justify-content: end;
    padding-top: 50%;
  }
}

.talents__image-wrapper {
  justify-content: center;
  align-items: center;
  width: 480px;
  height: 640px;
  display: flex;
  overflow: hidden;
}

@media (width >= 769px) and (width <= 1024px) {
  .talents__image-wrapper {
    height: 608px;
  }
}

@media (width <= 768px) {
  .talents__image-wrapper {
    height: 488px;
  }
}

@media (width <= 426px) {
  .talents__image-wrapper {
    height: 264px;
  }
}

@media (width <= 320px) {
  .talents__image-wrapper {
    height: 234px;
  }
}

.talents__image-wrapper img {
  object-fit: contain;
  width: 80%;
  height: auto;
  display: block;
  box-shadow: -44px -44px 0 -20px #ec5028, 0 -44px 0 -20px #ec5028, -44px 0 0 -20px #ec5028;
}

@media (width <= 426px) {
  .talents__image-wrapper img {
    box-shadow: -36px -33px 0 -20px #ec5028, 0 -33px 0 -20px #ec5028, -36px 0 0 -20px #ec5028;
  }
}

.visually-covered {
  white-space: nowrap !important;
  clip-path: inset(100%) !important;
  clip: rect(0 0 0 0) !important;
  border: 0 !important;
  width: 1px !important;
  height: 1px !important;
  margin: -1px !important;
  padding: 0 !important;
  position: absolute !important;
  overflow: hidden !important;
}

@media (width >= 1025px) and (width <= 1439px) {
  .covered-tablet {
    display: none !important;
  }

  .visible-tablet {
    display: none;
  }
}

.covered {
  display: none;
}

.mobile-overlay {
  z-index: 1000;
  background-color: #222324;
  border: none;
  border-radius: 8px;
  flex-direction: column;
  align-items: center;
  width: 100vw;
  height: 100vh;
  margin: 0;
  padding: 20px;
  display: none;
  position: fixed;
  inset: 0;
  overflow: hidden;
}

@media (width <= 426px) {
  .mobile-overlay {
    display: flex;
  }
}

.mobile-overlay:modal {
  max-height: 100vh;
}

.mobile-overlay__close-button-wrapp {
  background: none;
  border: none;
  justify-content: space-between;
  align-items: center;
  width: 100%;
  display: flex;
}

.mobile-overlay__logo {
  width: 96px;
  height: auto;
}

.mobile-overlay__body {
  width: 100%;
  padding: 0 32px;
}

@media (width <= 426px) {
  .mobile-overlay__body {
    padding: 80px 0 0;
  }
}

.mobile-overlay__list {
  flex-direction: column;
  justify-content: center;
  align-items: center;
  row-gap: 32px;
  margin: 0;
  padding: 0;
  list-style: none;
  display: flex;
}

@media (width <= 426px) {
  .mobile-overlay__list {
    row-gap: 24px;
  }
}

.mobile-overlay__item {
  text-align: center;
  width: 100%;
}

.mobile-overlay__item--burger {
  margin: -24px 0;
}

.mobile-overlay__link {
  color: #fff;
  justify-content: center;
  align-items: center;
  font-size: 24px;
  font-weight: 600;
  text-decoration: none;
  transition: transform .3s, color .3s;
  display: inline-flex;
  position: relative;
}

.mobile-overlay__link:hover {
  color: #ed3000;
  transform: scale(1.05);
}

.mobile-overlay__link:hover:after {
  width: 100%;
}

.mobile-overlay__link:after {
  content: "";
  background-color: #ed3000;
  border-radius: 4px;
  width: 0;
  height: 2px;
  transition: width .3s;
  position: absolute;
  bottom: -4px;
  left: 50%;
  transform: translateX(-50%);
}

@media (width <= 426px) {
  .mobile-overlay__link {
    font-size: 24px;
  }
}

.mobile-overlay:has(.page__menu:target) {
  overflow: hidden;
}

.mobile-overlay .cross-button {
  background-color: #121212;
  border: none;
  justify-content: center;
  align-items: center;
  width: 24px;
  height: 24px;
  margin: 0 30px;
  padding: 0;
  display: flex;
}

.mobile-overlay .cross-button:hover {
  background-color: #ed3000;
  border-radius: 8%;
}

.mobile-overlay .cross-button:hover:before, .mobile-overlay .cross-button:hover:after {
  background-color: #fff;
}

.mobile-overlay .cross-button:before, .mobile-overlay .cross-button:after {
  content: "";
  background-color: #adb5bd;
  border-radius: 4px;
  width: 18px;
  height: 2px;
  position: absolute;
}

.mobile-overlay .cross-button:before {
  transform: rotate(45deg);
}

.mobile-overlay .cross-button:after {
  transform: rotate(-45deg);
}

.burger-button {
  background-color: #121212;
  width: 24px;
  height: 24px;
  margin-left: auto;
  margin-right: 16px;
  display: none;
  position: relative;
}

@media (width <= 426px) {
  .burger-button {
    display: block;
  }
}

.burger-button span, .burger-button:before, .burger-button:after {
  content: "";
  background-color: #adb5bd;
  border-radius: 2px;
  width: 100%;
  height: 3px;
  transition: transform .3s, opacity .3s;
  position: absolute;
  left: 0;
}

.burger-button span {
  top: 50%;
  transform: translateY(-50%);
}

.burger-button:before {
  top: 0;
}

.burger-button:after {
  bottom: 0;
}

.burger-button:hover span, .burger-button:hover:before, .burger-button:hover:after {
  background-color: #ed3000;
}

@media (width <= 426px) {
  .button__burger-menu {
    display: block;
  }
}

.get-started {
  text-align: center;
  padding: 0 50px 96px;
}

@media (width <= 768px) {
  .get-started {
    padding: 0 20px 56px;
  }
}

@media (width <= 426px) {
  .get-started {
    padding: 0 16px 36px;
  }
}

.get-started__title {
