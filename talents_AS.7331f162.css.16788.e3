:root {
  --mobile-max: 425px;
  --tablet-min: 768px;
  --desktop-min: 1024px;
  --large-desktop: 1440px;
  --container-width: 1200px;
  --container-padding-x: 40px;
  --font-family-poppins: "Poppins", sans-serif;
  --h1-size: 128px;
  --h1-line-height: .8;
  --h1-letter-spacing: -.03em;
  --h1-font-weight: 600;
  --h2-size: 86px;
  --h2-line-height: 1;
  --h2-letter-spacing: 0;
  --h2-font-weight: 400;
  --h3-size: 48px;
  --h3-line-height: 1;
  --h3-letter-spacing: 0;
  --h3-font-weight: 400;
  --h4-size: 36px;
  --h4-line-height: 1.3;
  --h4-letter-spacing: 0;
  --h4-font-weight: 400;
  --h5-size: 17px;
  --h5-line-height: 1.3;
  --h5-letter-spacing: 0;
  --h5-font-weight: 400;
  --main-text-size: 18px;
  --main-text-line-height: 1.3;
  --main-text-letter-spacing: 0;
  --button-size: 16px;
  --button-line-height: 1.1;
  --button-letter-spacing: 0;
  --input-size: 16px;
}

body {
  font-family: var(--font-family-poppins);
  margin: 0;
  padding: 0;
}

h1 {
  font-size: var(--h1-size);
  line-height: var(--h1-line-height);
  letter-spacing: var(--h1-letter-spacing);
  font-weight: var(--h1-font-weight);
}

h2 {
  font-size: var(--h2-size);
  line-height: var(--h2-line-height);
  letter-spacing: var(--h2-letter-spacing);
  font-weight: var(--h2-font-weight);
}

h3 {
  font-size: var(--h3-size);
  line-height: var(--h3-line-height);
  letter-spacing: var(--h3-letter-spacing);
  font-weight: var(--h3-font-weight);
}

h4 {
  font-size: var(--h4-size);
  line-height: var(--h4-line-height);
  letter-spacing: var(--h4-letter-spacing);
  font-weight: var(--h4-font-weight);
}

h5 {
  font-size: var(--h5-size);
  line-height: var(--h5-line-height);
  letter-spacing: var(--h5-letter-spacing);
  font-weight: var(--h5-font-weight);
}

p {
  font-size: var(--main-text-size);
  line-height: var(--main-text-line-height);
  letter-spacing: var(--main-text-letter-spacing);
  margin-block-start: 0;
}

button {
  font-size: var(--button-size);
  line-height: var(--button-line-height);
  letter-spacing: var(--button-letter-spacing);
  font-family: var(--font-family-poppins);
}

html {
  color: #fff;
  background-color: #121212;
  font-family: Poppins, Helvetica Neue, Helvetica, Arial, sans-serif;
}

body.dialog-open {
  overflow: hidden;
}

.top-bar {
  justify-content: space-between;
  align-items: center;
  padding: 0 40px 0 0;
  display: flex;
}

@media (width <= 768px) {
  .top-bar {
    padding: 0;
  }
}

@media (width <= 426px) {
  .top-bar {
    justify-content: space-between;
    align-items: center;
    padding: 0 20px 0 0;
    display: flex;
  }
}

.top-bar__logo {
  width: 156px;
  height: auto;
}

@media (width <= 768px) {
  .top-bar__logo {
    width: 112px;
    height: auto;
  }
}

@media (width <= 426px) {
  .top-bar__logo {
    width: 96px;
    height: auto;
  }
}

.top-bar__logo {
  transition: transform .3s;
}

.top-bar__logo:hover {
  transform: scale(1.2);
}

@media (width <= 426px) {
  .top-bar__nav {
    display: none;
  }
}

.top-bar__nav ul {
  gap: 32px;
  margin: 0;
  padding: 0;
  list-style: none;
  display: flex;
}

.top-bar__nav a {
  text-transform: uppercase;
  color: #fff;
  font-size: 18px;
  font-weight: 400;
  text-decoration: none;
  transition: color .3s;
}

@media (width >= 769px) and (width <= 1024px) {
  .top-bar__nav a {
    font-size: 14px;
  }
}

@media (width <= 768px) {
  .top-bar__nav a {
    font-size: 12px;
  }
}

@media (width <= 456px) {
  .top-bar__nav a {
    font-size: 10px;
  }
}

.top-bar__nav a:hover {
  text-decoration: underline;
}

.top-bar__nav .user-menu {
  position: relative;
}

.top-bar__nav .user-menu:hover .user-menu__dropdown, .top-bar__nav .user-menu:focus-within .user-menu__dropdown {
  display: flex;
}

.top-bar__nav .user-menu .photographer-link {
  color: #fff;
  background-color: #ec5028;
  border-radius: 30px;
  padding: 8px 16px;
  font-weight: 500;
  text-decoration: none;
  transition: background-color .3s, transform .2s;
}

.top-bar__nav .user-menu .photographer-link:hover {
  background-color: #ed3000;
  text-decoration: none;
  transform: translateY(-2px);
}

.top-bar__nav .user-menu .photographer-link:focus {
  background-color: #e68268;
}

.top-bar__nav .user-menu__dropdown {
  background-color: #222324;
  border-radius: 20px;
  flex-direction: column;
  gap: 12px;
  min-width: 204px;
  margin-top: 8px;
  padding: 32px 24px;
  display: none;
  position: absolute;
  top: 100%;
  right: 0;
  box-shadow: 0 4px 12px #121212;
}

@media (width >= 769px) and (width <= 1024px) {
  .top-bar__nav .user-menu__dropdown {
    min-width: 180px;
    padding: 24px 16px;
    font-size: 14px;
  }
}

@media (width <= 768px) {
  .top-bar__nav .user-menu__dropdown {
    min-width: 144px;
    padding: 24px 16px;
    font-size: 12px;
  }
}

.top-bar__nav .user-menu__link {
  color: #fff;
  font-size: var(--button-size);
  cursor: pointer;
  text-align: center;
  background-color: #222324;
  border: 2px solid #ec5028;
  border-radius: 30px;
  margin-left: 8px;
  padding: 8px 16px;
  transition: background-color .3s, color .3s;
}

@media (width >= 769px) and (width <= 1024px) {
  .top-bar__nav .user-menu__link {
    padding: 4px 8px;
    font-size: 14px;
  }
}

@media (width <= 768px) {
  .top-bar__nav .user-menu__link {
    padding: 4px 8px;
    font-size: 12px;
  }
}

.top-bar__nav .user-menu__link--log {
  color: #ec5028;
  text-align: left;
  padding: 0 0 0 16px;
  text-decoration: underline;
}

.top-bar__nav .user-menu__link--log:hover {
  color: #ed3000;
  text-align: left;
  text-decoration: none;
}

.top-bar__nav .user-menu__link--log:focus {
  color: #e68268;
  text-align: left;
  text-decoration: none;
}

.top-bar__nav .user-menu__link:focus {
  color: #fff;
  background-color: #e68268;
  border: 2px solid #e68268;
  outline: none;
  text-decoration: none;
}

.top-bar__nav .user-menu__link:hover {
  color: #fff;
  background-color: #ed3000;
  outline: none;
  text-decoration: none;
}

.container {
  margin-left: auto;
  margin-right: auto;
  padding: 20px 20px 28px;
}

@media (width >= 1025px) and (width <= 1439px) {
  .container {
    padding: 20px 40px 32px 0;
  }
}

@media (width <= 768px) {
  .container {
    padding: 20px 40px 36px 0;
  }
}

@media (width <= 426px) {
  .container {
    align-items: center;
    padding: 24px 16px 24px 0;
    display: flex;
  }
}

.footer {
  color: #fff;
  background-color: #222324;
  justify-content: center;
  align-items: center;
  padding: 75px 40px;
  display: flex;
