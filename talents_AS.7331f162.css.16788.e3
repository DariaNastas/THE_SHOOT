}

@media (width <= 426px) {
  .footer-right .footer-links {
    text-align: center;
    flex-flow: wrap;
    order: 1;
    gap: 16px;
    width: 100%;
  }
}

.footer-right .footer-links li a {
  color: #fff;
  font-size: 24px;
  text-decoration: none;
}

@media (width >= 769px) and (width <= 1024px) {
  .footer-right .footer-links li a {
    font-size: 21px;
  }
}

@media (width <= 768px) {
  .footer-right .footer-links li a {
    font-size: 18px;
  }
}

@media (width <= 426px) {
  .footer-right .footer-links li a {
    font-size: 16px;
  }
}

@media (width <= 320px) {
  .footer-right .footer-links li a {
    font-size: 16px;
  }
}

.footer-right .footer-links li a:hover {
  text-decoration: underline;
}

.footer-title {
  color: #adb5bd;
  margin: 0;
  font-size: 158px;
  font-weight: bold;
}

@media (width >= 1025px) and (width <= 1439px) {
  .footer-title {
    font-size: 128px;
  }
}

@media (width >= 769px) and (width <= 1024px) {
  .footer-title {
    font-size: 96px;
  }
}

@media (width <= 768px) {
  .footer-title {
    font-size: 84px;
  }
}

@media (width <= 426px) {
  .footer-title {
    font-size: 46px;
  }
}

@media (width <= 320px) {
  .footer-title {
    font-size: 44px;
  }
}

.header {
  z-index: 2;
  background-color: #121212;
  align-items: center;
  width: 100%;
  padding: 0 20px;
  position: fixed;
  top: 0;
}

@media (width >= 1025px) and (width <= 1439px) {
  .header {
    padding: 0 40px;
  }
}

@media (width <= 768px) {
  .header {
    padding: 0 16px;
  }
}

.talents {
  justify-content: space-between;
  padding: 96px 40px 100px;
  display: flex;
}

@media (width <= 768px) {
  .talents {
    padding: 80px 16px 96px;
  }
}

@media (width <= 426px) {
  .talents {
    padding: 34px 0 34px 16px;
  }
}

.talents__content {
  max-width: 50%;
}

.talents__title {
  margin: 0 0 8px;
}

@media (width <= 768px) {
  .talents__title {
    font-size: 36px;
  }
}

@media (width <= 426px) {
  .talents__title {
    font-size: 24px;
