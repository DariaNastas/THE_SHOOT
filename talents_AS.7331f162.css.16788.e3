}

.talents__subtitle {
  margin: 0;
  padding: 0 0 40px;
}

@media (width <= 768px) {
  .talents__subtitle {
    padding: 0 0 32px;
  }
}

@media (width <= 426px) {
  .talents__subtitle {
    padding: 0 0 24px;
  }
}

.talents__list {
  margin: 0;
  padding: 0;
  list-style: none;
}

.talents__list-item {
  font-size: var(--h3-size);
  line-height: var(--h3-line-height);
  letter-spacing: var(--h3-letter-spacing);
  font-weight: var(--h3-font-weight);
  color: #fff;
  cursor: pointer;
  margin: 0 0 40px;
  text-decoration: none;
}

.talents__list-item:hover {
  color: #ed3000;
  text-decoration: underline;
}

.talents__list-item:active {
  color: #ec5028;
  text-decoration: none;
}

.talents__list-item--active {
  color: #ec5028;
}

@media (width >= 769px) and (width <= 1024px) {
  .talents__list-item {
    font-size: 36px;
  }
}

@media (width <= 768px) {
  .talents__list-item {
    margin: 0 0 24px;
    font-size: 24px;
  }
}

@media (width <= 426px) {
  .talents__list-item {
    margin: 0 0 24px;
