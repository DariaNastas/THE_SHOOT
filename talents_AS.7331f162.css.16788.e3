    font-size: 17px;
  }
}

@media (width <= 426px) {
  .get-started__description {
    font-size: 12px;
  }
}

.get-started__steps {
  grid-template-rows: repeat(3, auto);
  grid-template-columns: 1fr auto 1fr;
  gap: 20px;
  display: grid;
  position: relative;
}

.get-started__image {
  grid-area: 1 / 2 / 4;
  place-self: center;
  max-width: 150px;
  height: auto;
  transform: translateX(16px);
}

.get-started__step {
  text-align: center;
  box-sizing: border-box;
  padding: 10px 0 20px;
}

.get-started__step h3 {
  margin: 0 0 4px;
}

@media (width <= 768px) {
  .get-started__step h3 {
    font-size: 36px;
  }
}

@media (width <= 426px) {
  .get-started__step h3 {
    font-size: 24px;
  }
}

@media (width <= 768px) {
  .get-started__step p {
    font-size: 14px;
  }
}

@media (width <= 426px) {
  .get-started__step p {
    font-size: 12px;
  }
}

.get-started__step--right {
  text-align: left;
  grid-area: 1 / 3;
}

.get-started__step--left {
  text-align: right;
  grid-area: 2 / 1;
}

.get-started__step--right-secondary {
  text-align: left;
  grid-area: 3 / 3;
}

.get-started__cta {
  color: #fff;
  cursor: pointer;
  background-color: #ec5028;
  border: none;
  border-radius: 30px;
  margin: 36px auto;
  padding: 8px 16px;
  font-size: 18px;
  text-decoration: none;
  display: inline-block;
  position: relative;
}

@media (width >= 769px) and (width <= 1024px) {
