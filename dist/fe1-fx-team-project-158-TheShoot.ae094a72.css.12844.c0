  cursor: pointer;
  background-color: #adb5bd;
  border-radius: 4px;
  justify-content: center;
  align-items: center;
  width: 100%;
  height: auto;
  font-size: 32px;
  display: flex;
}

.profile-form__photo-slot input {
  display: none;
}

.profile-form__button {
  justify-content: right;
  gap: 16px;
  margin: 0 0 96px;
  display: flex;
}

.profile-form__button--cancel {
  color: #fff;
  font-size: var(--button-size);
  cursor: pointer;
  background-color: #222324;
  border: 2px solid #ec5028;
  border-radius: 30px;
  width: 206px;
  height: 48px;
  padding: 5px 15px;
  transition: background-color .3s, color .3s;
}

.profile-form__button--cancel:focus {
  background-color: #e68268;
  border-color: #e68268;
  outline: none;
}

.profile-form__button--cancel:hover {
