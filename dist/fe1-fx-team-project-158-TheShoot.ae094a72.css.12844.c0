}

.profile-form__button--create {
  color: #fff;
  cursor: pointer;
  background-color: #ec5028;
  border: none;
  border-radius: 30px;
  width: 206px;
  height: 48px;
  padding: 8px 16px;
  font-size: 16px;
  text-decoration: none;
  transition: background-color .3s;
}

.profile-form__button--create:hover {
  background-color: #ed3000;
}

.profile-form__button--create:focus {
  background-color: #e68268;
}

.auth__description {
  text-align: left;
  color: #adb5bd;
  margin: 0;
  padding: 84px 40px 24px;
  text-decoration: none;
}

.main-talents__title {
  margin: 0;
  padding: 0 40px;
}

.portfolio__grid {
  grid-template-rows: auto auto auto;
  grid-template-columns: 1fr 1fr 1fr;
  gap: 20px;
  margin: 24px 40px 96px;
  display: grid;
}

.portfolio__description {
  color: #fff;
  grid-area: 1 / 1 / 2 / 2;
  font-size: 1rem;
  line-height: 1.6;
}

.portfolio__tags {
  flex-wrap: wrap;
  grid-area: 2 / 1 / 3 / 2;
  gap: 16px;
  display: flex;
}

.portfolio__tag {
  color: #fff;
  cursor: pointer;
  font-size: var(--button-size);
  background-color: #222324;
  border: 1px solid #adb5bd;
  border-radius: 20px;
  flex: none;
  padding: 8px 16px;
}

.portfolio__tag:hover {
  background-color: #adb5bd;
}

.portfolio__image-wrapper {
  grid-area: 1 / 2 / 4 / 3;
  justify-content: center;
  align-items: center;
  width: 100%;
  display: flex;
  position: relative;
  overflow: hidden;
}

.portfolio__image {
  object-fit: contain;
  width: 80%;
  height: auto;
  margin: 24px 0 0 24px;
  display: block;
  box-shadow: -44px -44px 0 -20px #ec5028, 0 -44px 0 -20px #ec5028, -44px 0 0 -20px #ec5028;
}

.portfolio__contact {
  text-align: left;
  grid-area: 3 / 3 / 4 / 4;
}

.portfolio__contact__location, .portfolio__contact__phone, .portfolio__contact__email, .portfolio__contact__price {
  color: #adb5bd;
  margin-bottom: 8px;
  font-size: 1rem;
}

.portfolio__button {
  color: #fff;
  cursor: pointer;
  background-color: #ec5028;
  border: none;
  border-radius: 30px;
  width: 128px;
  height: 32px;
  padding: 8px;
  font-size: 16px;
  text-decoration: none;
  transition: background-color .3s;
}

.portfolio__button:hover {
  background-color: #ed3000;
}

.portfolio__button:focus {
  background-color: #e68268;
}

.profile-form__gallery {
  padding: 0 0 96px 40px;
}

.profile-form__gallery--title {
  color: #484b4e;
  margin: 0 0 24px 40px;
}

.profile-form__photo-slot--img {
  width: 440px;
  height: 544px;
}

.burger-button {
  width: 24px;
  height: 24px;
  margin-left: 32px;
  margin-right: auto;
  display: none;
  position: relative;
}

@media (width <= 426px) {
  .burger-button {
    display: block;
  }
}

.burger-button span, .burger-button:before, .burger-button:after {
  content: "";
  background-color: #adb5bd;
  border-radius: 2px;
  width: 100%;
  height: 3px;
  transition: transform .3s, opacity .3s;
  position: absolute;
  left: 0;
}

.burger-button span {
  top: 50%;
  transform: translateY(-50%);
}

.burger-button:before {
  top: 0;
}

.burger-button:after {
  bottom: 0;
}

.burger-button:hover span, .burger-button:hover:before, .burger-button:hover:after {
  background-color: #ed3000;
}

.mobile-overlay {
  background-color: #484b4e;
  border: none;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  width: 100%;
  height: 100%;
  margin: 0;
  display: flex;
}

.mobile-overlay:modal {
  max-width: 100%;
  max-height: 100%;
}

.mobile-overlay__close-button-wrapp {
  display: flex;
  position: relative;
  top: 24px;
}

.mobile-overlay__body {
  width: 100%;
  padding: 0 32px;
}

@media (width <= 426px) {
  .mobile-overlay__body {
    padding: 80px 0 0;
  }
}

.mobile-overlay__list {
  flex-direction: column;
  justify-content: center;
  align-items: center;
  row-gap: 32px;
  margin: 0;
  padding: 0;
  list-style: none;
  display: flex;
}

@media (width <= 426px) {
  .mobile-overlay__list {
    row-gap: 24px;
  }
}

.mobile-overlay__item {
  text-align: center;
  width: 100%;
}

.mobile-overlay__link {
  color: #fff;
  justify-content: center;
  align-items: center;
  font-size: 24px;
  font-weight: 600;
  text-decoration: none;
  transition: transform .3s, color .3s;
  display: inline-flex;
  position: relative;
}

@media (width <= 426px) {
  .mobile-overlay__link {
    font-size: 18px;
  }
}

.mobile-overlay__link:hover {
  color: #ed3000;
  transform: scale(1.05);
}

.mobile-overlay__link:hover:after {
  width: 100%;
}

.mobile-overlay__link:after {
  content: "";
  background-color: #ed3000;
  border-radius: 4px;
  width: 0;
  height: 2px;
  transition: width .3s;
  position: absolute;
  bottom: -4px;
  left: 50%;
  transform: translateX(-50%);
}

.mobile-overlay:has(.page__menu:target) {
  overflow: hidden;
}

.cross-button {
  background-color: #121212;
  justify-content: center;
  align-items: center;
  margin: 0 40px;
  display: flex;
  position: absolute;
  top: 10px;
  right: 10px;
}

.cross-button:hover {
  background-color: #ed3000;
  border-radius: 8%;
}

.cross-button:hover:before, .cross-button:hover:after {
  background-color: #484b4e;
}

.cross-button:before, .cross-button:after {
  content: "";
  background-color: #484b4e;
  border-radius: 4px;
  width: 18px;
  height: 2px;
  position: absolute;
}

.cross-button:before {
  transform: rotate(45deg);
}

.cross-button:after {
  transform: rotate(-45deg);
}
