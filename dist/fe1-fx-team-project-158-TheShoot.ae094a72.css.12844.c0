}

.profile-form__photo-upload {
  cursor: pointer;
  text-align: center;
  color: #484b4e;
  background-color: #adb5bd;
  border-radius: 4px;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  gap: 12px;
  width: 675px;
  height: 704px;
  font-size: 12px;
  display: flex;
}

.profile-form__photo-upload input {
  display: none;
}

.profile-form__upload-icon {
  justify-content: center;
  align-items: center;
  width: 72px;
  height: 72px;
  display: flex;
}

.profile-form__upload-icon img {
  object-fit: contain;
  width: 100%;
  height: 100%;
}

.profile-form__fields {
  flex-direction: column;
  flex-grow: 1;
  gap: 16px;
  display: flex;
}

.profile-form__text {
  color: #adb5bd;
  margin: 0;
  font-size: 12px;
}

.profile-form__input, .profile-form__textarea {
  color: #adb5bd;
  width: 96% s;
  background-color: #222324;
  border: 1px solid #484b4e;
  border-radius: 30px;
  height: 24px;
  padding: 12px 16px;
}

.profile-form__input::placeholder, .profile-form__textarea::placeholder {
